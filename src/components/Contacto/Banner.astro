



---
import Container from "../Container.astro";
import Wave from "../Wave.astro";
---

<section  data-aos="zoom-out" class="bg-Contacto bg-fixed h-[70vh] bg-cover ">
    <div class="h-full bg-gradient-to-r from-blue-400/85 to-cyan-200/80 content-center">
        <Container>

            <h1
                data-aos="fade-up"
                data-aos-delay="100"
                class="text-center text-6xl lg:text-7xl whitespace-normal mb-8 montserrat-bold text-white"
            >
                Contacto
            </h1>

        </Container>
        <Wave></Wave>
    </div>
</section>
